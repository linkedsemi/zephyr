
#include <zephyr/dt-bindings/gpio/gpio.h>
#include <mem.h>

#include <zephyr/dt-bindings/memory-attr/memory-attr.h>
#include <zephyr/dt-bindings/memory-attr/memory-attr-riscv.h>

/ {
    #address-cells = <1>;
    #size-cells = <1>;

    cpus {
        #address-cells = <1>;
        #size-cells = <0>;
        cpu0: cpu@0 {
            device_type = "cpu";
            compatible = "linkedsemi,riscv";
            riscv,isa = "rv32imafcxp";
            reg = <0>;
        };
    };

    sram0: memory@10000000{
        compatible = "mmio-sram";
        reg = <0x10000000 DT_SIZE_K(512)>;
    };

    sram1: memory@10080000{
        compatible = "mmio-sram";
        reg = <0x10080000 DT_SIZE_K(768)>;
    };

/*
    share_memory0: memory@1013f000{
        compatible = "mmio-sram";
        reg = <0x1013f000 0x1000>;
    };
*/

    soc {
        compatible = "simpe-bus";
        #address-cells = <1>;
        #size-cells = <1>;
        interrupt-parent = <&intc>;

        intc: interrupt-controller {
            compatible = "linkedsemi,riscv,e906-intc";
            #address-cells = <0>;
            #interrupt-cells = <2>;
            interrupt-controller;
        };

        flash_controller: flash-controller@40000000 {
            compatible = "linkedsemi,ls-flash-controller";
            reg = <0x40000000 0x400>;

            #address-cells = <1>;
            #size-cells = <1>;
            interrupts = <3 3>;

            status = "disabled";

            flash0: flash@8000000 {
                compatible = "soc-nv-flash";
                reg = <0x8000000 0x80000>;
                erase-block-size = <4096>;
                write-block-size = <4>;
            };
        };

        pinctrl: pin-controller@4001f180 {
            compatible = "linkedsemi,lsqsh-pinctrl";
            reg = <0x4001f180 0x200>;
            interrupts = <19 3>;
            interrupt-names = "EXTI_GPIO_VCore";
            #address-cells = <1>;
            #size-cells = <1>;

            gpioa: gpio@4001f180 {
                compatible = "linkedsemi,lsqsh-gpio";
                gpio-controller;
                #gpio-cells = <2>;
                reg = <0x4001f180 0x20>;
                status = "disabled";
            };

            gpiob: gpio@4001f1a0 {
                compatible = "linkedsemi,lsqsh-gpio";
                gpio-controller;
                #gpio-cells = <2>;
                reg = <0x4001f1a0 0x20>;
                status = "disabled";
            };

            gpioc: gpio@4001f1c0 {
                compatible = "linkedsemi,lsqsh-gpio";
                gpio-controller;
                #gpio-cells = <2>;
                reg = <0x4001f1c0 0x20>;
                status = "disabled";
            };

            gpiod: gpio@4001f1e0 {
                compatible = "linkedsemi,lsqsh-gpio";
                gpio-controller;
                #gpio-cells = <2>;
                reg = <0x4001f1e0 0x20>;
