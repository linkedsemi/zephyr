
#include <zephyr/dt-bindings/gpio/gpio.h>

/ {
    #address-cells = <1>;
    #size-cells = <1>;

    cpus {
        #address-cells = <1>;
        #size-cells = <0>;
        cpu0: cpu@0 {
            device_type = "cpu";
            compatible = "linkedsemi,riscv";
            riscv,isa = "rv32imafcxp";
            reg = <0>;
        };
    };

    sram0: memory@10000000{
        compatible = "mmio-sram";
        reg = <0x10000000 0x80000>;
    };

    sram1: memory@10080000{
        compatible = "mmio-sram";
        reg = <0x10080000 0xc0000>;
    };

    soc {
        compatible = "simpe-bus";
        #address-cells = <1>;
        #size-cells = <1>;
        interrupt-parent = <&intc>;

        intc: interrupt-controller {
            compatible = "linkedsemi,riscv,e906-intc";
            #address-cells = <0>;
            #interrupt-cells = <2>;
            interrupt-controller;
        };

        flash_controller: flash-controller@40000000 {
            compatible = "linkedsemi,ls-flash-controller";
            reg = <0x40000000 0x400>;

            #address-cells = <1>;
            #size-cells = <1>;
            interrupts = <3 3>;

            status = "disabled";

            flash0: flash@8000000 {
                compatible = "soc-nv-flash";
                reg = <0x8000000 0x80000>;
                erase-block-size = <4096>;
                write-block-size = <4>;
            };
        };

        mtimer: machine-timer@e0004000 {
            compatible = "linkedsemi,machine-timer";
            reg = <0xe0004000 0x8000>;
            interrupts = <7 0>;
        };

        i2c1: i2c@40083000 {
            compatible = "linkedsemi,ls-i2c";
            reg = <0x40083000 0x400>;
            #address-cells = <1>;
            #size-cells = <0>;
            interrupts = <66 3>;
            status = "disabled";
        };

        i2c2: i2c@40083400 {
            compatible = "linkedsemi,ls-i2c";
            reg = <0x40083400 0x400>;
            #address-cells = <1>;
            #size-cells = <0>;
            interrupts = <67 3>;
            status = "disabled";
        };

        i2c3: i2c@40083800 {
            compatible = "linkedsemi,ls-i2c";
            reg = <0x40083800 0x400>;
            #address-cells = <1>;
            #size-cells = <0>;
            interrupts = <68 3>;
            status = "disabled";
        };

        i2c4: i2c@40083c00 {
            compatible = "linkedsemi,ls-i2c";
            reg = <0x40083c00 0x400>;
            #address-cells = <1>;
            #size-cells = <0>;
            interrupts = <69 3>;
            status = "disabled";
        };

        i2c5: i2c@40084000 {
            compatible = "linkedsemi,ls-i2c";
            reg = <0x40084000 0x400>;
            #address-cells = <1>;
            #size-cells = <0>;
            interrupts = <50 3>;
            status = "disabled";
        };

        i2c6: i2c@40084400 {
            compatible = "linkedsemi,ls-i2c";
            reg = <0x40084400 0x400>;
            #address-cells = <1>;
            #size-cells = <0>;
            interrupts = <51 3>;
            status = "disabled";
        };

        i2c7: i2c@40084800 {
            compatible = "linkedsemi,ls-i2c";
            reg = <0x40084800 0x400>;
            #address-cells = <1>;
            #size-cells = <0>;
            interrupts = <52 3>;
            status = "disabled";
        };

        i2c8: i2c@40084C00 {
            compatible = "linkedsemi,ls-i2c";
            reg = <0x40084C00 0x400>;
            #address-cells = <1>;
            #size-cells = <0>;
            interrupts = <53 3>;
            status = "disabled";
        };

        i2c9: i2c@40085000 {
            compatible = "linkedsemi,ls-i2c";
            reg = <0x40085000 0x400>;
            #address-cells = <1>;
            #size-cells = <0>;
            interrupts = <54 3>;
            status = "disabled";
        };

        i2c10: i2c@40085400 {
            compatible = "linkedsemi,ls-i2c";
            reg = <0x40085400 0x400>;
            #address-cells = <1>;
            #size-cells = <0>;
            interrupts = <55 3>;
            status = "disabled";
        };

        i2c11: i2c@40085800 {
            compatible = "linkedsemi,ls-i2c";
            reg = <0x40085800 0x400>;
            #address-cells = <1>;
            #size-cells = <0>;
            interrupts = <56 3>;
            status = "disabled";
        };

        i2c12: i2c@40085C00 {
            compatible = "linkedsemi,ls-i2c";
            reg = <0x40085C00 0x400>;
            #address-cells = <1>;
            #size-cells = <0>;
            interrupts = <57 3>;
            status = "disabled";
        };

        i2c13: i2c@40086000 {
            compatible = "linkedsemi,ls-i2c";
            reg = <0x40086000 0x400>;
            #address-cells = <1>;
            #size-cells = <0>;
            interrupts = <58 3>;
            status = "disabled";
        };

        i2c14: i2c@40086400 {
            compatible = "linkedsemi,ls-i2c";
            reg = <0x40086400 0x400>;
            #address-cells = <1>;
            #size-cells = <0>;
            interrupts = <59 3>;
            status = "disabled";
        };

        smbus1: smbus1 {
            compatible = "linkedsemi,smbus";
            #address-cells = <1>;
            #size-cells = <0>;
            i2c = <&i2c1>;
            status = "disabled";
        };

        smbus2: smbus2 {
            compatible = "linkedsemi,smbus";
            #address-cells = <1>;
            #size-cells = <0>;
            i2c = <&i2c2>;
            status = "disabled";
        };

        smbus3: smbus3 {
            compatible = "linkedsemi,smbus";
            #address-cells = <1>;
            #size-cells = <0>;
            i2c = <&i2c3>;
            status = "disabled";
        };

        smbus4: smbus4 {
            compatible = "linkedsemi,smbus";
            #address-cells = <1>;
            #size-cells = <0>;
            i2c = <&i2c4>;
            status = "disabled";
        };

        smbus5: smbus5 {
            compatible = "linkedsemi,smbus";
            #address-cells = <1>;
            #size-cells = <0>;
            i2c = <&i2c5>;
            status = "disabled";
        };

        smbus6: smbus6 {
            compatible = "linkedsemi,smbus";
            #address-cells = <1>;
            #size-cells = <0>;
            i2c = <&i2c6>;
            status = "disabled";
        };

        smbus7: smbus7 {
            compatible = "linkedsemi,smbus";
            #address-cells = <1>;
            #size-cells = <0>;
            i2c = <&i2c7>;
            status = "disabled";
        };

        smbus8: smbus8 {
            compatible = "linkedsemi,smbus";
            #address-cells = <1>;
            #size-cells = <0>;
            i2c = <&i2c8>;
            status = "disabled";
        };

        smbus9: smbus9 {
            compatible = "linkedsemi,smbus";
            #address-cells = <1>;
            #size-cells = <0>;
            i2c = <&i2c9>;
            status = "disabled";
        };

        smbus10: smbus10 {
            compatible = "linkedsemi,smbus";
            #address-cells = <1>;
            #size-cells = <0>;
            i2c = <&i2c10>;
            status = "disabled";
        };

        smbus11: smbus11 {
            compatible = "linkedsemi,smbus";
            #address-cells = <1>;
            #size-cells = <0>;
            i2c = <&i2c11>;
            status = "disabled";
        };

        smbus12: smbus12 {
            compatible = "linkedsemi,smbus";
            #address-cells = <1>;
            #size-cells = <0>;
            i2c = <&i2c12>;
            status = "disabled";
        };

        smbus13: smbus13 {
            compatible = "linkedsemi,smbus";
            #address-cells = <1>;
            #size-cells = <0>;
            i2c = <&i2c13>;
            status = "disabled";
        };

        smbus14: smbus14 {
            compatible = "linkedsemi,smbus";
            #address-cells = <1>;
            #size-cells = <0>;
            i2c = <&i2c14>;
            status = "disabled";
        };

        uart1: uart@40087000 {
            compatible = "linkedsemi-uart";
            reg = <0x40087000 0x400>;
            interrupts = <94 3> ;
            status = "disabled";
        };

        uart2: uart@40087400 {
            compatible = "linkedsemi-uart";
            reg = <0x40087400 0x400>;
            interrupts = <95 3> ;
            status = "disabled";
        };

        uart3: dwuart@40087800 {
            compatible = "ns16550";
            reg = <0x40087800 0x400>;
            clock-frequency = <32000000>;
            interrupts = <96 3> ;
            reg-shift = <2>;
            status = "disabled";
        };

        uart4: dwuart@40087c00 {
            compatible = "ns16550";
            reg = <0x40087c00 0x400>;
            clock-frequency = <32000000>;
            interrupts = <97 3> ;
            reg-shift = <2>;
            status = "disabled";
        };

        uart5: dwuart@40088000 {
            compatible = "ns16550";
            reg = <0x40088000 0x400>;
            clock-frequency = <32000000>;
            interrupts = <98 3> ;
            reg-shift = <2>;
            status = "disabled";
        };

        uart6: dwuart@40088400 {
            compatible = "ns16550";
            reg = <0x40088400 0x400>;
            clock-frequency = <32000000>;
            interrupts = <99 3> ;
            reg-shift = <2>;
            status = "disabled";
        };
    };

    power-states {
        idle: idle{
            compatible = "zephyr,power-state";
            power-state-name = "runtime-idle";
        };

        lp0: lp0 {
            compatible = "zephyr,power-state";
            power-state-name = "standby";
        };
    };
};
