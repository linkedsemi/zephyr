        uart1: uart@40087000 {
            compatible = "linkedsemi,ls-uart";
            reg = <0x40087000 0x400>;
            interrupts = <94 3> ;
            status = "disabled";
        };

        uart2: uart@40087400 {
            compatible = "linkedsemi,ls-uart";
            reg = <0x40087400 0x400>;
            interrupts = <95 3> ;
            status = "disabled";
        };

        uart3: dwuart@40087800 {
            compatible = "ns16550";
            reg = <0x40087800 0x400>;
            clock-frequency = <50000000>;
            interrupts = <96 3> ;
            reg-shift = <2>;
            status = "disabled";
        };

        uart4: dwuart@40087c00 {
            compatible = "ns16550";
            reg = <0x40087c00 0x400>;
            clock-frequency = <50000000>;
