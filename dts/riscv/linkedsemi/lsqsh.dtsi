                gpio-controller;
                #gpio-cells = <2>;
                reg = <0x4001f340 0x20>;
                status = "disabled";
            };
        };

        mtimer: machine-timer@e0004000 {
            compatible = "linkedsemi,machine-timer";
            reg = <0xe0004000 0x8000>;
            interrupts = <7 0>;
        };

