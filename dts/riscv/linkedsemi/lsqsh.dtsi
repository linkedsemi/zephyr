        mac: ethernet@4001c000 {
            compatible = "linkedsemi,ethernet", "snps,designware-ethernet";
            reg = <0x4001c000 0x2000>;
            interrupts = <40 3>;
            status = "disabled";
        };

        peci0: peci@4008c400 {
            compatible = "linkedsemi,ls-peci";
            reg = <0x4008c400 0x400>;
            #address-cells= <1>;
            #size-cells = <0>;
            interrupts = <132 3>;
            status = "disabled";
        };

        sdhci: sdhci@40009000 {
            compatible = "linkedsemi,sdhci";
            reg = <0x40009000 0x1000>;
            interrupts = <41 3>;
            max-bus-freq = <50000000>;
            min-bus-freq = <200000>;
            status = "disabled";
        };
